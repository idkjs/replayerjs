{
  "name": "replayerjs",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "reason-react": "^0.9.1",
    "@smartcuts/reason-antd": "https://github.com/smartcuts/reason-antd.git",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.4",
    "@testing-library/user-event": "^13.5.0",
    "antd": "^4.19.3",
    "bs-css-emotion": "^4.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "5.0.0",
    "rescript-classnames": "^6.0.0",
    "rrweb": "^1.1.2",
    "rrweb-snapshot": "^1.1.13",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "watch": "bsb -make-world -w",
